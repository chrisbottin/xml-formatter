{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2016
    },
    "rules": {
        "quotes": ["error", "single"],
        "no-var": "error",
        "prefer-const": "error"
    }
}
